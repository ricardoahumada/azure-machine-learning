    "import requests\n",
    "\n",
    "# URL de la API\n",
    "url = \"http://127.0.0.1:5000/predict\"\n",
    "\n",
    "# Datos de entrada\n",
    "data = {\"features\": [1.1, 2.5, 1.1, 1.2]}\n",
    "\n",
    "# Enviar solicitud POST\n",
    "response = requests.post(url, json=data)\n",
    "\n",
    "# Mostrar la respuesta\n",
    "print(response.json())"
   ]
  }
